package aplicativo;

import java.util.Date;

public class VotosDenuncias {
	private int votoID;
    private int userID;
    private int denunciaID;
    private String voto;
    private Date datavoto;
    
    public int getVotoID() {
        return votoID;
    }

    public void setVotoID(int votoID) {
        this.votoID = votoID;
    }

    public int getUserID() {
        return userID;
    }

    public void setUserID(int userID) {
        this.userID = userID;
    }

    public int getDenunciaID() {
        return denunciaID;
    }

    public void setDenunciaID(int denunciaID) {
        this.denunciaID = denunciaID;
    }

    public String getVoto() {
        return voto;
    }

    public void setVoto(String voto) {
        this.voto = voto;
    }

    public Date getDataHoraVoto() {
        return dataHoraVoto;
    }

    public void setDataHoraVoto(Date dataHoraVoto) {
        this.dataHoraVoto = dataHoraVoto;
    }
}
