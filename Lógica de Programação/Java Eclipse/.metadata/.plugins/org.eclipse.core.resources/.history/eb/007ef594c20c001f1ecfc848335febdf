package aula;

public class ContaEspecial extends Conta {
	public float limite;
	
	//método construtor
	public ContaEspecial(int _num, String _cpf, float _saldo, float _limite) {
		super(_num, _cpf, _saldo); // chamando o método construtor da classe Pai
		this.setLimite(limite);
	}
	
	public void setLimite(float _lim) {
		this.limite = _lim;
	}
	public float getLimite() {
		return this.limite;
	}
	
	public void ConsultaContaEspecial() {
		this.ConsultaConta();
		System.out.println("Limite da Conta: " + limite)
	}
}
