package aplicativo;

import java.util.Date*;

public class Denuncias {
	private int denunciaID;
    private int userID;
    private String descricao;
    private String tipoproblema;
    private String status;
    private Date datachamado;
    private Date dataatualização;
    private float custoOS;
    
    public int getDenunciaID() {
        return denunciaID;
    }
    public void setDenunciaID(int denunciaID) {
        this.denunciaID = denunciaID;
    }
    public int getUserID() {
        return this.userID;
    }
    public void setUserID(int userID) {
        this.userID = userID;
    }
    public String getDescricao() {
        return this.descricao;
    }
    public void setDescricao(String descricao) {
        this.descricao = descricao;
    }

    public String getTipoproblema() {
        return this.tipoproblema;
    }

    public void setTipoproblema(String tipoproblema) {
        this.tipoproblema = tipoproblema;
    }
    
    public String getStatus() {
        return this.status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public Date getDataHora() {
        return dataHora;
    }

    public void setDataHora(Date dataHora) {
        this.dataHora = dataHora;
    }
    
    public float getCustoOS() {
        return this.custoOS;
    }

    public void setCustoOS(String custoOS) {
        this.custoOS = custoOS;
    }
}
